<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title – приложение ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/scripts/jquery-3.4.1.js")
    @Scripts.Render("~/scripts/Script.js")
    @Scripts.Render("~/scripts/bootstrap-slider.min.js")
    <script src="~/Scripts/jquery-3.4.1.js" type="text/javascript"></script>
    <script language="javascript" type="text/javascript">
        $("#btn_add").click(function () {
            $("#card-1").clone().insertAfter("#card-1");
        })
    </script>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupport">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a href="/Home/Index" class="nav-link">Действия</a>
                </li>
                <li class="nav-item">
                    <a href="/Home/Sphere" class="nav-link">Сферы</a>
                </li>
                <li class="nav-item">
                    <a href="/Goal/Index" class="nav-link">Цели</a>
                </li>
            </ul>
            @Html.Partial("_LoginPartial")
        </div>
    </nav>
    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
